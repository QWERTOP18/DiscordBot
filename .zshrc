alias mkpydirs='for i in {1..10}; do mkdir py_problem_set$(printf "%03d" $i); done'
alias mkpy='for i in {1..10}; do touch py_problem$(printf "%03d" $i).py; done'
alias mkjs='for i in {1..10}; do touch js_problem$(printf "%03d" $i).js; done'
alias mkc='for i in {1..10}; do touch c_problem$(printf "%03d" $i).c; done'

